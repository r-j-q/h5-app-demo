
.firstNameRow {
	display: flex;
	flex-direction: row;
	align-items: center;
	margin-top: 40rpx;
	border-radius: 12rpx;
}
.my-uni-input {
	margin: 10rpx auto;
	width: 500rpx;
	height: 90rpx;
	border: none;
	outline: none;
	padding-left: 20rpx;
	font-size: 38rpx;
	font-weight: bold;
	color: #333;
	-webkit-background-color: #fff;
	background-color: #fff;
	caret-color: #cdcdcd;
}
 .my-uni-input:focus {
	-webkit-box-shadow: 0 0 0 1000px whiteinset;
	background-color: #fff;
	outline: none !important;
}
.firstName {
	width: 32rpx;
	height: 36rpx;
	margin-left: 20rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.firstNameX {
	width: 16rpx;
	height: 16rpx;
	margin-left: 20rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.firstB {
	width: 100%;
	height: 100%
}
.uniInput {
	background-color: #2F243A;
	border: none;
	color: #fff;
	font-size: 38rpx;
}

