<scroll-view class="{{(('wuc-tab')+' '+tabClass)}}" style="{{(tabStyle)}}" scroll-with-animation="{{true}}" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><block a:if="{{!textFlex}}"><view class="_div"><view class="{{(('wuc-tab-item _div')+' '+(index===tabCur?selectClass+' cur':''))}}" id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',[index,'$event']]]]]}}" onTap="__e" a:for="{{tabList}}" a:for-item="item" a:for-index="index" a:key="index"><text class="{{(item.icon)}}"></text><label class="_span">{{item.name}}</label></view></view></block><block a:if="{{textFlex}}"><view class="flex text-center _div"><view class="{{(('wuc-tab-item flex-sub _div')+' '+(index===tabCur?selectClass+' cur':''))}}" id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',[index,'$event']]]]]}}" onTap="__e" a:for="{{tabList}}" a:for-item="item" a:for-index="index" a:key="index"><text class="{{(item.icon)}}"></text><label class="_span">{{item.name}}</label></view></view></block></scroll-view>